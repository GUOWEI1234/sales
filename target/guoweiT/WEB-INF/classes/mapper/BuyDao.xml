<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.my.dao.BuyDao">

    <insert id="insertBuy" parameterType="com.my.pojo.Buy">
        insert into buy (buy_price,user_id,product_id,buy_count) VALUES (#{buy_price},#{user_id},#{product_id},#{buy_count})
    </insert>

</mapper>